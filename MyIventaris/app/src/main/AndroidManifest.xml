<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.ac.unsyiah.myiventaris">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/iventaris"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyIventaris">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".RegisterActivity"/>
        <activity android:name=".MainActivity"/>


        <activity
            android:name=".BuatBarang"
            android:label="@string/title_activity_buatbarang" />
        <activity
            android:name=".LihatBarang"
            android:label="@string/title_activity_lihatbarang" />
        <activity
            android:name=".UpdateBarang"
            android:label="@string/title_activity_updatebarang" />
    </application>

</manifest>